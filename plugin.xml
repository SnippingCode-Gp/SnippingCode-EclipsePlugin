<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
         
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="GUITest.actionSet">
         <menu
               label="Snipping &amp;Code"
               id="sampleMenu">
            <separator
                  name="login">
            </separator>
            <separator
                  name="signUp">
            </separator>
            <separator
                  name="import">
            </separator>
            <separator
                  name="export">
            </separator>
         </menu>
		<action
		       label="&amp;Login"
		       class="guitest.actions.SampleAction"
		       menubarPath="sampleMenu/login"
		       id="login">
		 </action>
		<action
		       label="&amp;Sign Up"
		       class="guitest.actions.SampleAction"
		       menubarPath="sampleMenu/signUp"
		       id="signUp">
		 </action>
		<action
		       label="&amp;Import Code"
		       class="guitest.actions.SampleAction"
		       menubarPath="sampleMenu/import"
		       id="import">
		 </action>
		<action
		       label="&amp;Export Code"
		       class="guitest.actions.SampleAction"
		       menubarPath="sampleMenu/export"
		       id="export">
		</action>
      </actionSet>
   </extension>

</plugin>
